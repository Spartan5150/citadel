<div class="form-group">
    <%= f.label :galaxy_id %>
    <%= f.text_field :galaxy_id, class: "form-control" %>
</div>
<div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
</div>
<div class="form-group">
    <%= f.label :address %>
    <%= f.text_field :address, class: "form-control" %>
</div>
<div class="form-group">
    <%= f.label :port %>
    <%= f.text_field :port, class: "form-control" %>
</div>
<div class="form-group">
    <%= f.label :pingport %>
    <%= f.text_field :pingport, class: "form-control" %>
</div>
<div class="form-group">
    <%= f.label :population %>
    <%= f.text_field :population, class: "form-control" %>
</div>

<div class="form-group float-right">
    <%= link_to "Cancel", admin_galaxies_path, {class: "btn btn-secondary", role: "button"} %>
    <%= f.submit "Submit", {class: "btn btn-primary", role: "button"} %>
</div>
